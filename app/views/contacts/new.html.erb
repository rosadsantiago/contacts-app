<h1>New Contacts</h1>
<%= form_tag "/contacts", method: "post" do %>
  
  <div class="form-group">
    First_name: <input type="text" class="form-control" name="form_first_name">
  </div>
  <div class="form-group">
    middle_name: <input type="text" class="form-control" name="form_middle_name">
  </div>
  <div class="form-group">
    Last_name: <input type="text" class="form-control" name="form_last_name">
  </div>
  <div class="form-group">
    Email: <input type="text" class="form-control" name="form_email">
  </div>
  <div class="form-group">
    Phone_number: <input type="text" class="form-control" name="form_phone_number">
  </div>
  <div class="form-group">
    Bio: <input  type="text" class="form-control" name="form_bio">
  </div>
  <input type="submit" class="btn btn-default" value="Create new contact">
<% end %>